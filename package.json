{
  "name": "bs-customizer",
  "version": "1.0.0",
  "description": "Create custom Bootstrap 4.x builds",
  "private": true,
  "scripts": {
    "deploy": "gh-pages --dist ./ --src \"{index.html,dist/**}\"",
    "dev": "webpack-dev-server --open --mode=development --config webpack.config.js",
    "prod": "webpack --mode=production --config webpack.config.js",
    "standard": "standard --parser babel-eslint",
    "vnu": "html-validator --file=index.html",
    "test": "npm run standard && npm run prod && npm run vnu"
  },
  "keywords": [
    "customizer",
    "bootstrap",
    "bootstrap4",
    "javascript"
  ],
  "author": "Johann-S <johann.servoire@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.2.1",
    "jquery": "^3.4.0",
    "jszip": "^3.1.5",
    "postcss": "^7.0.6",
    "sass.js": "^0.10.13",
    "uglifyjs-browser": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "gh-pages": "^2.0.1",
    "glob-all": "^3.1.0",
    "html-validator-cli": "^4.1.4",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.4.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "standard": "^12.0.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "standard": {
    "ignore": [
      "src/js/lib/*.js"
    ]
  },
  "engines": {
    "node": ">=10"
  }
}
