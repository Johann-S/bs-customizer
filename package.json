{
  "name": "bs-customizer",
  "version": "1.4.0",
  "description": "Create custom Bootstrap 4.x builds",
  "private": true,
  "scripts": {
    "deploy": "gh-pages --dist ./ --src \"{index.html,dist/**}\"",
    "dev": "webpack-dev-server --open --mode=development --config webpack.config.js",
    "prod": "webpack --mode=production --config webpack.config.js",
    "standard": "standard --parser babel-eslint",
    "vnu": "html-validator --file=index.html",
    "test": "npm run standard && npm run prod && npm run vnu"
  },
  "keywords": [
    "customizer",
    "bootstrap",
    "bootstrap4",
    "javascript"
  ],
  "author": "Johann-S <johann.servoire@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.7.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "jszip": "^3.2.2",
    "postcss": "^7.0.21",
    "sass.js": "^0.11.1",
    "uglifyjs-browser": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.7.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "gh-pages": "^2.1.1",
    "glob-all": "^3.1.0",
    "html-validator-cli": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "standard": "^14.3.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "standard": {
    "ignore": [
      "src/js/lib/*.js"
    ]
  },
  "engines": {
    "node": ">=10"
  }
}
